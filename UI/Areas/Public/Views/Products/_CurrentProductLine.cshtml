@model ProductViewModel
@{

}
@foreach (var product in Model.ProductModels)
{
    <tr class=" b-nep-tbres-tr-original " id="@product.Id"
    data-price-add-break-line="false" data-parent-group-guid="" style="">
    <td style="padding: 0"></td>
    <td class="b-nep-mstd-1-w2" style="background-color: white; " data-not-selectable="">
        <div class="b-nep-mstd-pic">
            <a href="#" rel=""
               role="goods-image-selector"
               style="background-image: url('@Model.ProductTemplateModel.DefaultPhotoUrl')"></a>
        </div>
        <ul class="b-nep-mstd-list-info">
            <li>
                <a href="/products/AYWIPARTS/AW1510739/info.html?price=2308"
                   target="_blank" class="b-nep-mstd-link-sett"
                   role="goods-info-fancybox-link"></a>
            </li>
            <li>
                    <a href="https://www.google.ru/search?q=@Model.ProductTemplateModel.VendorCode @Model.ProductTemplateModel.ManufacturerName&tbm=isch"
                   class="b-nep-mstd-link-g" target="_blank"></a>
            </li>
        </ul>
    </td>
        <td class="b-nep-mstd-3" style="" data-make-name="@Model.ProductTemplateModel.ManufacturerName">
        <div class="b-nep-mstd-nmcontainer">
            <div class="b-nep-mstd-pname-wrapper">
                <a class="b-nep-mstd-pname c-minfo-link-v2" href="#c-minfo"
                   data-href="make_name_description" style="">
                    @Model.ProductTemplateModel.ManufacturerName
                </a>
            </div>
            <a asp-action="Details" asp-area="Public" asp-controller="Products" asp-route-query="@Model.ProductTemplateModel.VendorCode"
                      target="_blank" class="b-nep-mstd-oem" style="">@Model.ProductTemplateModel.VendorCode</a>
        </div>
        <div class="b-nep-mstd-sdescript search-detail-name-crop" title="@Model.ProductTemplateModel.Name" style="">
                @Model.ProductTemplateModel.Name
        </div>
    </td>
    <td class="b-nep-mstd-4">
            <div class="b-nep-mstd-sdescript search-detail-name-crop" title="@Model.ProductTemplateModel.Name" style="">
                @Model.ProductTemplateModel.Name
        </div>
    </td>
    <td class="b-nep-mstd-5" style="">
        <ul class="b-nep-mstd-list-info">
            <li>
                <a href="#"
                   target="_blank" class="b-nep-mstd-link-sett"
                   role="goods-info-fancybox-link"></a>
            </li>
            <li>
                    <a href="https://www.google.ru/search?q=@Model.ProductTemplateModel.VendorCode @Model.ProductTemplateModel.ManufacturerName&tbm=isch"
                   class="b-nep-mstd-link-g" target="_blank"></a>
            </li>
            <!--<li><a class="b-nep-mstd-link-note" href=""></a></li>-->
        </ul>
    </td>
    <td class="b-nep-mstd-6" style="">
        <div class="b-nep-mstd-mob-th">Склад</div>
        <div class="b-nep-mstd-store" style="">
            <a href="#"
               data-href="/prices/2/description" class="price-name"
               role="price-description-form" style=""> MSK </a>
        </div>
    </td>
    <td class="b-nep-mstd-7" data-price-id="2" style="">
        <div class="b-nep-mstd-mob-th">Стат.</div>
        <ul class="b-nep-mstd-list-stat">
            <li>
                <div class="b-nep-tstat stat-icon stat-bad" data-stat="0"
                     data-placement="bottom"> </div>
            </li>
        </ul>
    </td>
    <td class="b-nep-mstd-weight" style=""></td>
    <td class="b-nep-mstd-8" style="">
        <div class="b-nep-mstd-mob-th">Кол.</div>
        <div class="b-nep-mstd-amount" style=""> @product.Amount </div>
    </td>
    <td class="b-nep-mstd-9" style="">
        <div class="b-nep-mstd-mob-th">Доставка</div>
        <div class="b-nep-mstd-delivery" style="">1-2дн.</div>
    </td>
    <td class="b-nep-mstd-10" style="">
        <div class="b-nep-mstd-mob-th">Цена</div>
        <div class="b-nep-mstd-price">
            <span class="sale-price-content" style="">
                @product.Price.ToString("N")
                <i>р.</i>
            </span>
        </div>
    </td>
    <td class="b-nep-mstd-11" style="">
        <a href="#" class="b-nep-add2bask price-item-bay-v2"
           data-toggle="modal"
           rel="#basket-form-dialog-v2" title="">В корзину</a>
    </td>
</tr>
}
